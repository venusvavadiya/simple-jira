{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "web",
  "projects": {
    "web": {
      "root": "apps/web",
      "sourceRoot": "apps/web/src",
      "projectType": "application",
      "prefix": "web",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/web",
            "main": "apps/web/src/main.ts",
            "tsConfig": "apps/web/tsconfig.app.json",
            "generatePackageJson": true
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "web:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/web"
          ],
          "options": {
            "jestConfig": "apps/web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "domain-project": {
      "root": "libs/domain-project",
      "sourceRoot": "libs/domain-project/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/domain-project/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/domain-project"
          ],
          "options": {
            "jestConfig": "libs/domain-project/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "frontend": {
      "projectType": "application",
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "targets": {
        "build": {
          "executor": "@nx-plus/vue:browser",
          "options": {
            "dest": "dist/apps/frontend",
            "index": "apps/frontend/public/index.html",
            "main": "apps/frontend/src/main.ts",
            "tsConfig": "apps/frontend/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "mode": "production",
              "filenameHashing": true,
              "productionSourceMap": true,
              "css": {
                "extract": true,
                "sourceMap": false
              }
            }
          }
        },
        "serve": {
          "executor": "@nx-plus/vue:dev-server",
          "options": {
            "browserTarget": "frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontend:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend/**/*.{ts,tsx,vue}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/frontend"
          ],
          "options": {
            "jestConfig": "apps/frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
